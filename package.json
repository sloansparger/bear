{
  "name": "@sloansparger/bear",
  "description": "Unofficial CLI for the Bear notes app (MacOS only)",
  "version": "0.1.2",
  "author": "Sloan Sparger @sloansparger",
  "license": "MIT",
  "repository": "sloansparger/bear",
  "homepage": "https://github.com/sloansparger/bear",
  "bugs": "https://github.com/sloansparger/bear/issues",
  "keywords": ["bear", "cli"],
  "main": "lib/index.js",
  "bin": {
    "bear": "./bin/run"
  },
  "dependencies": {
    "@oclif/command": "^1.6.1",
    "@oclif/config": "^1.15.1",
    "@oclif/errors": "^1.2.2",
    "@oclif/plugin-autocomplete": "^0.2.0",
    "@oclif/plugin-commands": "^1.2.3",
    "@oclif/plugin-help": "^3.0.1",
    "@oclif/plugin-update": "^1.3.10",
    "conf": "^6.2.4",
    "tslib": "^1.13.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.6",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.24",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "eslint-config-prettier": "^6.11.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.3"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": ["/bin", "/lib", "/npm-shrinkwrap.json", "/oclif.manifest.json"],
  "types": "lib/index.d.ts",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "bear",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-commands",
      "@oclif/plugin-update"
    ]
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "copy-xcall": "cp -r ./src/utils/xcall/xcall.app lib/utils/xcall && cp ./src/utils/xcall/xcall-license.txt ./lib/utils/xcall/xcall-license.txt",
    "prepack": "rm -rf lib && tsc -b && npm run copy-xcall && oclif-dev manifest && oclif-dev readme",
    "test": "echo 'TODO'",
    "version": "oclif-dev readme && git add README.md"
  }
}
